// Super simple Hello World CeruleanASM Program
// Main is where the program starts executing from (and must be global)
    @global main
main:
    lli       r0, 'H'
    putchar   r0
    lli       r0, 'e'
    putchar   r0
    lli       r0, 'l'
    putchar   r0
    lli       r0, 'l'
    putchar   r0
    lli       r0, 'o'
    putchar   r0
    lli       r0, ','
    putchar   r0
    lli       r0, ' '
    putchar   r0
    lli       r0, 'W'
    putchar   r0
    lli       r0, 'o'
    putchar   r0
    lli       r0, 'r'
    putchar   r0
    lli       r0, 'l'
    putchar   r0
    lli       r0, 'd'
    putchar   r0
    lli       r0, '!'
    putchar   r0
    lli       r0, '\n'
    putchar   r0
    halt
