
function int32 min(int32 a, int32 b) {
    if (a <= b)
        return a;
    return b;
}

function int32 max(int32 a, int32 b) {
    if (a >= b)
        return a;
    return b;
}

function int32 main() {
    int32 x = 10;

    // Conditionals look and work the same as C/C++
    if (x > 10) {
        println ("x > 10");
    } elif (x == 10) {
        println ("x == 10");
    } else { // x < 10
        println ("x < 10");
    }

    println (min (7, 3));
    println (max (7, 3));

    return 0;
}