// //         sub     rsp, 4                   // isNegative [rbp -  8] [int 4-bytes]
// //         sub     rsp, 4                   // digit      [rbp - 12] [int 4-bytes]
// //         sub     rsp, 4                   // char       [rbp - 16] [int 4-bytes]
// //         sub     rsp, 4                   // numDigits  [rbp - 20] [int 4-bytes]

// //         // initialize local vars
// //         mov     edx, dword [rbp + 16]
// //         mov     dword [rbp - 4], edx
// //         mov     dword [rbp - 8], 0
// //         mov     dword [rbp - 12], 0
// //         mov     dword [rbp - 16], 0
// //         mov     dword [rbp - 20], 0

// //         // account for negative numbers
// //         // if num < 0:
// //         //   isNegative = 1
// //         //   num = -num
// //         cmp     dword [rbp - 4], 0
// //         jge     positive0
// //         mov     dword [rbp - 8], 1
// //         neg     dword [rbp - 4]
// // positive0: nop

// //         // while num > 9:
// // while0:
// //         cmp     dword [rbp - 4], 9 
// //         jle     endwhile0 
// //         //   digit = num % 10
// //         //   num = num / 10
// //         mov     edi, 10
// //         mov     eax, dword [rbp - 4]
// //         cdq     
// //         idiv    edi
// //         mov     dword [rbp - 12], edx  // digit = num % 10
// //         mov     dword [rbp - 4], eax   // num = num / 10

// //         //   char = digit + '0'
// //         mov     eax, dword [rbp - 12]
// //         add     eax, '0'
// //         //   push char
// //         push    rax
// //         //   numDigits++ 
// //         inc     dword [rbp - 20]
// //         // repeat
// //         jmp     while0
// // endwhile0: 

// //         //   char = num + '0'
// //         mov     eax, dword [rbp - 4]
// //         add     eax, '0'
// //         //   push char
// //         push    rax
// //         //   numDigits++ 
// //         inc     dword [rbp - 20]
        
// //         // if isNegative == 1:
// //         cmp     dword [rbp - 8], 1
// //         jne     positive1
// //         //   push '-'
// //         push    '-'
// //         //   numDigits++
// //         inc     dword [rbp - 20]

// // positive1: 
// //         nop
        
// //         // while numDigits > 0:
// // while1:
// //         cmp     dword [rbp - 20], 0
// //         jle     endwhile1
// //         //   pop char
// //         pop     rdx
// //         //   printchar char
// //         push    rdx   // arg0
// //         call    print__char 
// //         pop     rdx    // arg0
// //         //   numDigits--
// //         dec     dword [rbp - 20]
// //         //   repeat 
// //         jmp     while1
// // endwhile1:

// //         mov     rsp, rbp                 // pop local vars 
// //         pop     rbp
// //         ret

