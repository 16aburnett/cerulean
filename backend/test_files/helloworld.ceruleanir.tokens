
GLOBAL GVARIABLE(@customary_greeting) ASSIGN IDENTIFIER(value) LPAREN TYPE_CHAR TIMES LPAREN STRING RPAREN RPAREN 
GLOBAL GVARIABLE(@length) ASSIGN IDENTIFIER(value) LPAREN TYPE_INT32 LPAREN INT RPAREN RPAREN 


FUNCTION TYPE_VOID GVARIABLE(@print_greeting) LPAREN TYPE_CHAR TIMES LPAREN LVARIABLE(%greeting) RPAREN COMMA TYPE_INT32 LPAREN LVARIABLE(%length) RPAREN RPAREN 
LBRACE 
LVARIABLE(%i) ASSIGN IDENTIFIER(value) LPAREN TYPE_INT32 LPAREN INT RPAREN RPAREN 
TYPE_LABEL IDENTIFIER(loop_start) 
IDENTIFIER(jge) LPAREN TYPE_INT32 LPAREN LVARIABLE(%i) RPAREN COMMA TYPE_INT32 LPAREN LVARIABLE(%length) RPAREN COMMA TYPE_LABEL LPAREN IDENTIFIER(loop_end) RPAREN RPAREN 
LVARIABLE(%char) ASSIGN IDENTIFIER(load) LPAREN TYPE_CHAR TIMES LPAREN LVARIABLE(%greeting) RPAREN COMMA TYPE_INT32 LPAREN LVARIABLE(%i) RPAREN RPAREN 
CALL GVARIABLE(@__builtin__print__char) LPAREN TYPE_CHAR LPAREN LVARIABLE(%char) RPAREN RPAREN 
LVARIABLE(%i) ASSIGN IDENTIFIER(add) LPAREN TYPE_INT32 LPAREN LVARIABLE(%i) RPAREN COMMA TYPE_INT32 LPAREN INT RPAREN RPAREN 
IDENTIFIER(jmp) LPAREN TYPE_LABEL LPAREN IDENTIFIER(loop_start) RPAREN RPAREN 
TYPE_LABEL IDENTIFIER(loop_end) 
CALL GVARIABLE(@__builtin__println) LPAREN RPAREN 
IDENTIFIER(return) LPAREN RPAREN 
RBRACE 


FUNCTION TYPE_INT32 GVARIABLE(@main) LPAREN RPAREN 
LBRACE 
CALL GVARIABLE(@print_greeting) LPAREN TYPE_CHAR TIMES LPAREN GVARIABLE(@customary_greeting) RPAREN COMMA TYPE_INT32 LPAREN GVARIABLE(@length) RPAREN RPAREN 
IDENTIFIER(return) LPAREN TYPE_INT32 LPAREN INT RPAREN RPAREN 
RBRACE 
END_OF_FILE 