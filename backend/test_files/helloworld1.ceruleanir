// Super simple hello world example with loading chars from a string
// Author: Amy Burnett
//========================================================================

function int32 @main () {
    block entry {
        %str_ptr = value (ptr("Hello, World!\n"))
        %i0 = value (int32(0))

        %c0 = load (type(char), ptr(%str_ptr), int32(%i0))
        call @__builtin__print__char (char(%c0))
        %i1 = add (int32(%i0), int32(1))

        %c1 = load (type(char), ptr(%str_ptr), int32(%i1))
        call @__builtin__print__char (char(%c1))
        %i2 = add (int32(%i1), int32(1))

        %c2 = load (type(char), ptr(%str_ptr), int32(%i2))
        call @__builtin__print__char (char(%c2))
        %i3 = add (int32(%i2), int32(1))

        %c3 = load (type(char), ptr(%str_ptr), int32(%i3))
        call @__builtin__print__char (char(%c3))
        %i4 = add (int32(%i3), int32(1))

        %c4 = load (type(char), ptr(%str_ptr), int32(%i4))
        call @__builtin__print__char (char(%c4))
        %i5 = add (int32(%i4), int32(1))

        %c5 = load (type(char), ptr(%str_ptr), int32(%i5))
        call @__builtin__print__char (char(%c5))
        %i6 = add (int32(%i5), int32(1))

        %c6 = load (type(char), ptr(%str_ptr), int32(%i6))
        call @__builtin__print__char (char(%c6))
        %i7 = add (int32(%i6), int32(1))

        %c7 = load (type(char), ptr(%str_ptr), int32(%i7))
        call @__builtin__print__char (char(%c7))
        %i8 = add (int32(%i7), int32(1))

        %c8 = load (type(char), ptr(%str_ptr), int32(%i8))
        call @__builtin__print__char (char(%c8))
        %i9 = add (int32(%i8), int32(1))

        %c9 = load (type(char), ptr(%str_ptr), int32(%i9))
        call @__builtin__print__char (char(%c9))
        %i10 = add (int32(%i9), int32(1))

        %c10 = load (type(char), ptr(%str_ptr), int32(%i10))
        call @__builtin__print__char (char(%c10))
        %i11 = add (int32(%i10), int32(1))

        %c11 = load (type(char), ptr(%str_ptr), int32(%i11))
        call @__builtin__print__char (char(%c11))
        %i12 = add (int32(%i11), int32(1))

        %c12 = load (type(char), ptr(%str_ptr), int32(%i12))
        call @__builtin__print__char (char(%c12))
        %i13 = add (int32(%i12), int32(1))

        %c13 = load (type(char), ptr(%str_ptr), int32(%i13))
        call @__builtin__print__char (char(%c13))

        return (int32(0))
    }
}







